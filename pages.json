{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "柒月贰柒",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "个人中心",
        "pullToRefresh":{
          "color":"#409eff",
          "offset":"56px"
        },
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/my/userInfo",
      "style": {
        "navigationBarTitleText": "用户信息",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/report/putin",
      "style": {
        "navigationBarTitleText": "入库报表",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/other/index",
      "style": {
        "navigationBarTitleText": "其他收支"
      }
    },
    {
      "path": "pages/login/logout",
      "style": {
        "navigationBarTitleText": "账号设置",
        "enablePullDownRefresh": false
      }
    }
  ],
  "tabBar": {
    "color": "#333333",
    "selectedColor": "#409eff",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/index/index",
      "iconPath": "static/img/new/index_4.png",
      "selectedIconPath": "static/img/new/index_3.png",
      "text": "首页"
    },
      {
        "pagePath": "pages/my/index",
        "selectedIconPath": "static/img/new/person_1.png",
        "iconPath": "static/img/new/person_0.png",
        "text": "我的"
      }
    ]
  },
  "globalStyle": {

    "rpxCalcMaxDeviceWidth": 600,
    "rpxCalcBaseDeviceWidth": 600,
    "rpxCalcIncludeWidth": 600,
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "柒月贰柒门户网站",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "onReachBottomDistance": 50,
    "navigationStyle": "custom",
    "pageOrientation": "auto",
    "mp-weixin": {
      "resizable": false
    }
  },
  "uniIdRouter": {}
}
